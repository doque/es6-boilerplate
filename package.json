{
  "name": "es6-boilerplate",
  "version": "0.0.1",
  "description": "Write node apps in ES6",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r babel-register",
    "lint": "eslint .",
    "test:watch": "mocha -r babel-register -w --watch-extensions js,jsx",
    "test:watch:all": "mocha  -r babel-register -w --reporter dot test/**/*.spec.js",
    "test:cov": "nyc npm run test",
    "test:cov:report": "nyc --reporter html npm run test",
    "test:cov:show": "open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doque/es6-boilerplate.git"
  },
  "keywords": [
    "es6",
    "node",
    "babel",
    "boilerplate"
  ],
  "author": "Dominique Busser <dombusser@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doque/es6-boilerplate/issues"
  },
  "homepage": "https://github.com/doque/es6-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "sinon": "^1.17.5"
  }
}
